{
  "default": {
    "vk": {
      "accessToken": "$VK_ACCESS_TOKEN",
      "version": "$VK_API_VERSION || 5.103",
      "domain": "jumoreski"
    },
    "aws": {
      "polly": {
        "voice": "Maxim",
        "outputFormat": "mp3"
      },
      "s3": {
        "bucketName": "jumoresques",
        "key": "jumoresques.mp3"
      },
      "accessKeyId": "$AWS_ACCESS_KEY_ID",
      "accessKey": "$AWS_ACCESS_KEY"
    }
  },
  "production": {
    "aws":{
      "s3": {
        "region": "us-west-1"
      }
    }
  }
}
